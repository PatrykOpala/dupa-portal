---
import BaseLayout from '../layouts/BaseLayout.astro';
import Header from '../components/Header.astro';
import ArticleBox from '../components/ArticleBox.astro';
import ArticleContainer from '../components/ArticleContainer.astro';
import ArticleSection from '../components/ArticleSection.astro';
---

<BaseLayout keywords="portal,portale,portal o niczym,o,niczym,portal internetowy,internetowy,nudy,nuda" 
			description="DUPA to portal o niczym, stworzony dla zabawy." title="DUPA - portal o niczym.">
	<Header />
	<ArticleSection sectionHeader="Artykuły">
		<ArticleContainer>
			<ArticleBox articleHeader="Chłodne dni" articleLink="./article/chlodne-dni"/>
			<ArticleBox articleHeader="Śmiech pod wiatr" articleLink="./article/smiech-pod-wiatr"/>
		</ArticleContainer>
	</ArticleSection>
</BaseLayout>
