---

<header class="portal-header">
    <img src="data:image/svg+xml;base64," alt="dupa logo" height="55">
    <nav></nav>
</header>

<style>
    .portal-header{
		width: 60vw;
		display: flex;
		flex-direction: row;
		justify-content: space-between;
		align-items: center;
		margin-inline: auto;
	}
	@media screen and (min-width: 600px) {
		.portal-header{
			width: 100%;
		}
		img{
			justify-content: start;
			height: 90px;
		}
	}
	@media screen and (min-width: 900px) {
		.portal-header{
			width: 72vw;
		}
	}
	@media screen and (max-width: 600px){
		.portal-header{
			width: 50vw;
		}
		img{
			height: 120px;
		}
	}

	@media screen and (max-width: 450px) {
		img{
			height: 80px;
		}
	}
</style>